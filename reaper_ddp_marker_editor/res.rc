IDM_EDITMARKERSMENU MENU
BEGIN
	POPUP ""
	BEGIN
		MENUITEM "Edit marker...",	IDC_EDIT_MARKER
		MENUITEM "Delete marker",	IDC_DELETE_MARKER
		MENUITEM SEPARATOR
		MENUITEM "Insert INDEX0 marker...",	IDC_INSERT_INDEX0_MARKER
		MENUITEM "Insert INDEX1 marker...",	IDC_INSERT_INDEX1_MARKER
		MENUITEM "Insert album marker...",	IDC_INSERT_ALBUM_MARKER
	END
END
